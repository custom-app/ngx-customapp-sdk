{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-customapp-jwt": {
      "projectType": "library",
      "root": "projects/jwt",
      "sourceRoot": "projects/jwt/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/jwt/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/jwt/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/jwt/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/jwt/src/test.ts",
            "tsConfig": "projects/jwt/tsconfig.spec.json",
            "karmaConfig": "projects/jwt/karma.conf.js"
          }
        }
      }
    },
    "ngx-customapp-errors": {
      "projectType": "library",
      "root": "projects/errors",
      "sourceRoot": "projects/errors/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/errors/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/errors/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/errors/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/errors/src/test.ts",
            "tsConfig": "projects/errors/tsconfig.spec.json",
            "karmaConfig": "projects/errors/karma.conf.js"
          }
        }
      }
    },
    "ngx-customapp-proto-http": {
      "projectType": "library",
      "root": "projects/proto-http",
      "sourceRoot": "projects/proto-http/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/proto-http/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/proto-http/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/proto-http/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/proto-http/src/test.ts",
            "tsConfig": "projects/proto-http/tsconfig.spec.json",
            "karmaConfig": "projects/proto-http/karma.conf.js"
          }
        }
      }
    },
    "ngx-customapp-pattern-auth-before-authorized-socket": {
      "projectType": "library",
      "root": "projects/pattern-auth-before-authorized-socket",
      "sourceRoot": "projects/pattern-auth-before-authorized-socket/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/pattern-auth-before-authorized-socket/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/pattern-auth-before-authorized-socket/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/pattern-auth-before-authorized-socket/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/pattern-auth-before-authorized-socket/src/test.ts",
            "tsConfig": "projects/pattern-auth-before-authorized-socket/tsconfig.spec.json",
            "karmaConfig": "projects/pattern-auth-before-authorized-socket/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "ngx-customapp-jwt",
  "cli": {
    "defaultCollection": "@ngrx/schematics"
  }
}
